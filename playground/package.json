{
  "name": "playground",
  "description": "This is the playground for nebula.",
  "version": "1.0.0",
  "author": "badboyku",
  "license": "MIT",
  "main": "src/index",
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-cli serve",
    "start:dist": "http-server ./dist",
    "build": "webpack --mode production",
    "clean:build": "rimraf ./dist",
    "clean:modules": "rimraf ./node_modules",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix",
    "prettier": "prettier --check \"./src/**/*.{css,scss,json}\"",
    "prettier:fix": "prettier --write \"./src/**/*.{css,scss,json}\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/runtime": "file:../node_modules/@babel/runtime",
    "@babel/runtime-corejs3": "file:../node_modules/@babel/runtime-corejs3",
    "@emotion/react": "file:../node_modules/@emotion/react",
    "@emotion/styled": "file:../node_modules/@emotion/styled",
    "@fontsource/material-icons": "file:../node_modules/@fontsource/material-icons",
    "@fontsource/roboto": "file:../node_modules/@fontsource/roboto",
    "@mui/material": "file:../node_modules/@mui/material",
    "core-js": "file:../node_modules/core-js",
    "nebula": "file:..",
    "react": "file:../node_modules/react",
    "react-dom": "file:../node_modules/react-dom"
  },
  "devDependencies": {
    "@babel/core": "file:../node_modules/@babel/core",
    "@babel/plugin-transform-runtime": "file:../node_modules/@babel/plugin-transform-runtime",
    "@babel/preset-env": "file:../node_modules/@babel/preset-env",
    "@babel/preset-react": "file:../node_modules/@babel/preset-react",
    "@babel/preset-typescript": "file:../node_modules/@babel/preset-typescript",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@svgr/webpack": "^8.1.0",
    "@types/nebula": "file:..",
    "@types/node": "file:../node_modules/@types/node",
    "@types/react": "file:../node_modules/@types/react",
    "@types/react-dom": "file:../node_modules/@types/react-dom",
    "babel-loader": "^10.0.0",
    "browserslist": "^4.25.4",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dotenv": "^17.2.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.4",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.5.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^8.1.1",
    "postcss-normalize": "^13.0.1",
    "postcss-preset-env": "^10.3.1",
    "react-refresh": "^0.17.0",
    "resolve": "^1.22.10",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.91.0",
    "sass-loader": "^16.0.5",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "typescript": "file:../node_modules/typescript",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-manifest-plugin": "^5.0.1"
  }
}
