{
  "name": "nebula",
  "description": "This is component library repo.",
  "version": "1.0.0",
  "author": "badboyku",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/badboyku/nebula.git"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "source": "src/index.ts",
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "scripts": {
    "prepare": "husky",
    "dev": "rollup -c --watch",
    "start": "NODE_ENV=development webpack-cli serve",
    "start:playground": "cd playground && npm run start",
    "storybook": "storybook dev -p 6006",
    "build:rollup": "rollup -c",
    "build:webpack": "webpack --mode production",
    "build:storybook": "storybook build",
    "clean:build": "rimraf ./dist ./playground/dist",
    "clean:modules": "rimraf ./node_modules ./package-lock.json ./playground/node_modules ./playground/package-lock.json",
    "clean:coverage": "rimraf ./test_coverage",
    "lint": "eslint \"./src/**/*.{js,ts}\"",
    "lint:fix": "eslint \"./src/**/*.{js,ts}\" --fix",
    "lint-staged": "TZ=UTC lint-staged",
    "prettier": "prettier --check \"./src/**/*.{css,scss,json}\"",
    "prettier:fix": "prettier --write \"./src/**/*.{css,scss,json}\"",
    "test": "TZ=UTC jest",
    "test:coverage": "TZ=UTC jest --coverage '--watchAll=false'",
    "test:watch": "TZ=UTC jest --watch"
  },
  "lint-staged": {
    "src/**/*.{css,scss,json}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "jest -u --findRelatedTests --passWithNoTests"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/material": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "dependencies": {
