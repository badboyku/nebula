    "@emotion/styled": "^11.14.1",
    "@fontsource/material-icons": "^5.2.5",
    "@fontsource/roboto": "^5.2.6",
    "@mui/material": "^7.3.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-image": "^3.0.3",
