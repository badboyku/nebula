{
  "name": "nebula",
  "description": "This is component library repo.",
  "version": "0.0.0",
  "author": "badboyku",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/badboyku/nebula.git"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "source": "src/index.ts",
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "scripts": {
    "prepare": "husky",
    "dev": "rollup -c --watch",
    "start:playground": "cd playground && npm run start",
    "start:storybook": "storybook dev -p 6006",
    "build:rollup": "rollup -c",
    "build:storybook": "storybook build",
    "clean:build": "rimraf ./dist ./playground/dist",
    "clean:modules": "rimraf ./node_modules ./playground/node_modules",
    "lint": "eslint \"./src/**/*.{js,ts}\"",
    "lint:fix": "eslint \"./src/**/*.{js,ts}\" --fix",
    "lint-staged": "TZ=UTC lint-staged",
    "prettier": "prettier --check \"./src/**/*.{css,scss,json}\"",
    "prettier:fix": "prettier --write \"./src/**/*.{css,scss,json}\""
  },
  "lint-staged": {
    "src/**/*.{css,scss,json}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/material": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.3",
    "@babel/runtime-corejs3": "^7.28.3",
    "core-js": "^3.45.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/material-icons": "^5.2.5",
    "@fontsource/roboto": "^5.2.6",
    "@mui/material": "^7.3.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@rollup/plugin-url": "^8.0.2",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-themes": "^8.6.14",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.6",
    "@storybook/react": "^8.6.14",
    "@storybook/react-webpack5": "^8.6.14",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "eslint-config-badboyku": "^1.0.42",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^8.6.14",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  }
}
